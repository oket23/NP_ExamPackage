<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>

		<PackageId>ExamPackage</PackageId>
		<Version>1.0.1</Version>
		<Authors>Oket13</Authors>
		<Company>OketSoft</Company>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/oket23/NP_Exam_Packages</RepositoryUrl>
		<Description>Exam packages</Description>
		<PackageTags>logger;serilog;shared;oket</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Serilog.Sinks.File" Version="7.0.0" />
		<None Include="README.md" Pack="true" PackagePath="" />
	</ItemGroup>

</Project>


